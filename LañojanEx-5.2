//this line include input output
#include <iostream>
//avoid writing std::
using namespace std;

//start of the execution of the program 
int main() {
    // Initialize a variable i to 10
    int i = 10;

    // While i >= 1, print i and decrement i by 1
    while (i >= 1) {
        cout << i << endl;
        i--;
    }

    return 0;
}
